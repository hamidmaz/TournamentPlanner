CREATE FUNCTION public."spMatches_GetByTournament"("tournamentId" integer) RETURNS SETOF public."Matches"
    LANGUAGE plpgsql
    AS $$BEGIN
RETURN QUERY
SELECT m.*
FROM "Matches" m
WHERE m."TournamentId" = "tournamentId"
ORDER By m."MatchRound" ASC;
END;
$$;


